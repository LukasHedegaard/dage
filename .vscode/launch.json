    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Training on Digits dataset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "args" : [
                "--method", "tune_source",
                "--architecture", "single_stream",
                "--source", "mnist",
                "--target", "mnist-m",
                "--model_base", "conv2",
                "--dense_size", "120",
                "--embed_size", "84",
                "--epochs", "30",
            ]
        },
        {
            "name": "Python: Domain Adaptation on Digits dataset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "args" : [
                "--method", "dage",
                "--architecture", "two_stream_pair_embeds",
                "--source", "mnist",
                "--target", "usps",
                "--model_base", "conv2",
                "--dense_size", "120",
                "--embed_size", "84",
                "--epochs", "30",
                "--batch_size", "128",
            ]
        },
        {
            "name": "Python: Domain Adaptation on Office dataset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "console": "integratedTerminal",
            "args" : [
                "--method", "ccsa",
                "--architecture", "two_stream_pair_embeds",
                "--source", "amazon",
                "--target", "webcam",
                "--model_base", "vgg16",
            ]
        },
    ]
}