    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Evaluate untrained",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "args": ["--mode", "test", "--method", "dsne"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Tune Source",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "args": ["--method", "tune_source"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Tune Both",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "args": ["--method", "tune_both"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Domain Adaption using CCSA",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "args": ["--method", "ccsa"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Domain Adaption using d-SNE",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "args": ["--method", "dsne"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Domain Adaption using Homebrew",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "args": ["--method", "homebrew"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Train on extracted features",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/run.py",
            "args": ["--features", "vgg16", "--model_base", "none", "--augment", "0", "--method", "dage_aux_dense"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}